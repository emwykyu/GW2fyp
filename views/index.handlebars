<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>The Guild Wars 2 Ley Line</title>
    <link rel="stylesheet" href="/stylesheets/app.css"/>
    <link rel="stylesheet" href="/stylesheets/style.css">
    <link rel="stylesheet" href="/stylesheets/demo.css">
    <link rel="stylesheet" href="/stylesheets/component1.css">
    <link rel="stylesheet" href="/stylesheets/normalize.css">
    <link rel="stylesheet" href="/Ionicons/css/ionicons.min.css">
    <script src="/modernizr/src/Modernizr.js"></script>
    <link href="http://fonts.googleapis.com/css?family=Lato:100,300,400,300italic" rel="stylesheet" type="text/css">
</head>
<body>

<form method="post" action="/users/login" class="login">
    {{#if user}}
        <p class="loggedInAs">logged in as {{user.username}}</p>
        <a href="/users/logout" class="button">Log Out</a>
        <a href="/myGW2" class="button buttonMygw2">MyGW2</a>
        <script type="text/javascript" src="http://www.youtube.com/player_api"></script>
        <script type="text/javascript">function toggleButtonPlayer_fhcIB(buttonobj){buttonobj.style.display="none";if(buttonobj.id=="gaf210codes-fhcIB-playbutton"){document.getElementById("gaf210codes-fhcIB-pausebutton").style.display="inline";}else{document.getElementById("gaf210codes-fhcIB-playbutton").style.display="inline";}}/*Youtube player code generated @ http://gaf210.imvustylez.net*/</script>
        <div class="gaf210codes_youtubebox">
            <iframe width="250" height="250" src="https://www.youtube.com/embed/QtvnP91PDfQ?enablejsapi=1&amp;fs=0&amp;rel=0&amp;showinfo=0&amp;modestbranding=1&amp;iv_load_policy=3&amp;controls=0&amp;autoplay=1&amp;loop=0&amp;playlist=QtvnP91PDfQ" onload="gaf210codes_fhcIB=new YT.Player(this)"></iframe>
        </div>
        <button type="button" class="playerbutton button" id="gaf210codes-fhcIB-playbutton" onclick="gaf210codes_fhcIB.playVideo();toggleButtonPlayer_fhcIB(this)" style="display:none">play</button>
        <button type="button" class="playerbutton button" id="gaf210codes-fhcIB-pausebutton" onclick="gaf210codes_fhcIB.pauseVideo();toggleButtonPlayer_fhcIB(this)">pause music</button>
    {{else}}
        <form method="post" action="/users/login" class="login">
            <div class="form-group">
                <input type="text" class="loginInput" name="username" placeholder="Username">
            </div>
            <div class="form-group">
                <input type="password" class="loginInput" name="password" placeholder="Password">
            </div>

            <button type="submit" class="button">Sign In</button>
            <a class="button" data-open="exampleModal1">Register</a>
            <!--<a href="/users/logout" class="button">Log Out</a>-->
        </form>
    {{/if}}
</form>


<div class="home">
</div>
<a href="/baseGame">
    <div class="" >
    <img src="/stylesheets/images/hotlogo.png" class="heroImg columns small-12 medium-12 large-row wow fade-in" data-wow-duration="2s" data-wow-delay="0s">

    </div>
</a>



<div class="reveal revealS" id="exampleModal1" data-reveal data-animation-in="fade-in" data-animation-out="fade-out">
    <h2 class="page-header">Register</h2>
    {{#if errors}}
        {{#each errors}}
            <div class="alert alert-danger">{{msg}}</div>
        {{/each}}
    {{/if}}
    <form method="post" action="/users/register" class="authForm">
        <!--<div class="form-group">-->
        <!--&lt;!&ndash;<label>Name</label>&ndash;&gt;-->
        <!--<input type="text" class="form-control" placeholder="Name" name="name">-->
        <!--</div>-->
        <div class="form-group">
            <!--<label>Username</label>-->
            <input type="text" class="form-control" placeholder="Username" name="username">
        </div>
        <div class="form-group">
            <!--<label>Email</label>-->
            <input type="text" class="form-control" placeholder="API Key" name="apiKey">
        </div>
        <div class="form-group">
            <!--<label>Password</label>-->
            <input type="password" class="form-control" placeholder="Password" name="password">
        </div>
        <div class="form-group">
            <!--<label>Confirm Password</label>-->
            <input type="password" class="form-control" placeholder="Confirm Password" name="password2">
        </div>
        <button type="submit" class="button">Submit</button>
    </form>
</div>

<script src="/jquery/dist/jquery.min.js"></script>
<script src="/foundation-sites/dist/foundation.min.js"></script>
<script src="/js/demo1.js"></script>
<script src="/js/polyfills.js"></script>
<script src="/js/app.js"></script>
<script src="/js/scroll.js"></script>
<script src="/js/wow.js"></script>
<!--<script src="/js/jput-2.js"></script>-->
<script>

    $(document).ready(function () {
        $(document).foundation();

        $(function () {
            new WOW().init();
        })

    });

</script>



</body>
</html>
